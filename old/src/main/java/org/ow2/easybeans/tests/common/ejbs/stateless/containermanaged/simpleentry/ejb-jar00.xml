    <!--  Bean -->
    <session>
      <description>SessionBean to test simple environment entry injection with the use of injection-target tag. When this tag is used, an env-entry-value is not required.</description>
      <display-name>SLSBSimpleEnvEntryInjectionByDescriptor</display-name>
      <ejb-name>SLSBSimpleEnvEntryInjectionByDescriptor</ejb-name>
      <ejb-class>org.ow2.easybeans.tests.common.ejbs.stateless.containermanaged.simpleentry.SLSBSimpleEnvEntryByDescriptor</ejb-class>
      <session-type>Stateless</session-type>
      <env-entry>
        <description>This entry test the injection defined by injection-target tag.</description>
        <env-entry-name>str00</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value>STRING00</env-entry-value>
        <injection-target>
        	<injection-target-class>org.ow2.easybeans.tests.common.ejbs.stateless.containermanaged.simpleentry.SLSBSimpleEnvEntryByDescriptor</injection-target-class>
        	<injection-target-name>strInjected</injection-target-name>
        </injection-target>
      </env-entry>
      <env-entry>
        <description>This entry must not have an specified value, so the container will not override the field value defined in the bean class.</description>
        <env-entry-name>StringEntry</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <injection-target>
        	<injection-target-class>org.ow2.easybeans.tests.common.ejbs.stateless.containermanaged.simpleentry.SLSBSimpleEnvEntryByDescriptor</injection-target-class>
        	<injection-target-name>strNotOverride</injection-target-name>
        </injection-target>
      </env-entry>
      </session>
