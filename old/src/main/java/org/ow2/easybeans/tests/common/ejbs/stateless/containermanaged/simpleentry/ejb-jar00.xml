  <enterprise-beans>
    <!--  Bean -->
    <session>
      <description>SessionBean to test simple environment entry injection.</description>
      <display-name>SLSBSimpleEnvEntryFieldInjection00</display-name>
      <ejb-name>SLSBSimpleEnvEntryFieldInjection00</ejb-name>
      <ejb-class>org.ow2.easybeans.tests.common.ejbs.stateless.containermanaged.simpleentry.SLSBSimpleEnvEntryFieldInjection00</ejb-class>
      <session-type>Stateless</session-type>
      <env-entry>
        <env-entry-name>str00</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value>STRING00</env-entry-value>
      </env-entry>
      <env-entry>
        <env-entry-name>strWithDefaultValue</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
      </env-entry>
      <env-entry>
        <env-entry-name>chr00</env-entry-name>
        <env-entry-type>java.lang.Character</env-entry-type>
        <env-entry-value>A</env-entry-value>
      </env-entry>
      <env-entry>
        <env-entry-name>int00</env-entry-name>
        <env-entry-type>java.lang.Integer</env-entry-type>
        <env-entry-value>12</env-entry-value>
      </env-entry>
      <env-entry>
        <env-entry-name>bol00</env-entry-name>
        <env-entry-type>java.lang.Boolean</env-entry-type>
        <env-entry-value>true</env-entry-value>
      </env-entry>
      <env-entry>
        <env-entry-name>dbl00</env-entry-name>
        <env-entry-type>java.lang.Double</env-entry-type>
        <env-entry-value>100</env-entry-value>
      </env-entry>
      <env-entry>
        <env-entry-name>bte00</env-entry-name>
        <env-entry-type>java.lang.Byte</env-entry-type>
        <env-entry-value>2</env-entry-value>
      </env-entry>
      <env-entry>
        <env-entry-name>shr00</env-entry-name>
        <env-entry-type>java.lang.Short</env-entry-type>
        <env-entry-value>200</env-entry-value>
      </env-entry>
      <env-entry>
        <env-entry-name>lng00</env-entry-name>
        <env-entry-type>java.lang.Long</env-entry-type>
        <env-entry-value>300</env-entry-value>
      </env-entry>
      <env-entry>
        <env-entry-name>org.ow2.easybeans.tests.common.ejbs.stateless.containermanaged.simpleentry.SLSBSimpleEnvEntryFieldInjection00/fltInjection</env-entry-name>
        <env-entry-type>java.lang.Float</env-entry-type>
        <env-entry-value>400</env-entry-value>
      </env-entry>
    </session>
